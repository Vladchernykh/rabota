#include <iostream>
#include <fstream>
#include <math.h>
#include <stdio.h>
using namespace std;

int main()
{
    int random, a, end;
    int i;
    ofstream fout;
    ifstream fin;
    fout.open("predel.txt");
    fout << "222";
    fout.close();
    fin.open("predel.txt");
    fin >> a;
    fin.close();
    for (end = 0; end <= a;) {
        cout << "enter chisla = ";
        cin >> random;
        cin >> i;
        end = i + random;
        cout << end << endl;
    }
}
